---
pagination:
  data: demos
  size: 1
  alias: demo
permalink: "demos/{{ demo.filename | slugify }}/"
layout: default
---

<div class="item">
  <h1>{{ demo.filename }}</h1>
  <div class="svg-container">
    <div>
      <h3>Before</h3>
      {{ demo.input.svg | safe }}
    </div>
    <div>
      <h3>After</h3>
      {{ demo.output.svg | safe }}
    </div>
  </div>
</div>
